{
  "version": 2,
  "outputDirectory": "dist/apps/nest-api/",
  "installCommand": "yarn install --frozen-lockfile",
  "buildCommand": "npx nx build nest-api",
  "builds": [
    {
      "src": "apps/nest-api/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/apps/nest-api/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
